<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">

    <Dialog
        id="valueHelpDialog"
        title="Select Name">

        <content>
            <List
                id="valueHelpList"
                items="{manifestModel>/Addressinfo}"
                mode="SingleSelectMaster">

                <StandardListItem
                    title="{manifestModel>name}"
                    description="{manifestModel>StreetNo}" />
            </List>
        </content>

        <beginButton>
            <Button text="OK" press="onValueHelpPress"/>
        </beginButton>

        <endButton>
            <Button text="Cancel" press="onCancelValueHelp"/>
        </endButton>

    </Dialog>
</core:FragmentDefinition>
